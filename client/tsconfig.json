{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "module": "ES2020",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    "target": "ES2021"
  },
  "exclude": ["node_modules"]
}
