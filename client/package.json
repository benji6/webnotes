{
  "version": "1.0.0",
  "repository": "https://github.com/benji6/webnotes",
  "author": "benji6",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheList",
    "build:js": "CACHE_LIST=CACHE_LIST parcel build --no-source-maps src/index.html",
    "build:robots": "touch dist/robots.txt",
    "fmt": "prettier --write '**/*'",
    "icons": "node scripts/icons",
    "start": "parcel src/index.html --open",
    "test": "run-p test:*",
    "test:audit": "yarn audit --level critical || [ $? -lt 16 ]",
    "test:fmt": "prettier --check '**/*'",
    "test:types": "tsc --noEmit"
  },
  "browserslist": [
    ">1.25%"
  ],
  "dependencies": {
    "@reach/router": "^1.3.1",
    "amazon-cognito-identity-js": "^4.5.5",
    "eri": "^25.0.4",
    "idb-keyval": "^3.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "@types/reach__router": "^1.3.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "favicons": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2"
  }
}
