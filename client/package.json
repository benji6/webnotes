{
  "version": "1.0.0",
  "repository": "https://github.com/benji6/webnotes",
  "author": "benji6",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheList",
    "build:js": "CACHE_LIST=CACHE_LIST parcel build --no-source-maps src/index.html",
    "build:robots": "touch dist/robots.txt",
    "format": "prettier --write 'src/**/*'",
    "icons": "node scripts/icons",
    "start": "parcel src/index.html",
    "test": "run-p test:*",
    "test:audit": "yarn audit --level high || [ $? -lt 8 ]",
    "test:format": "prettier --check 'src/**/*'",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@reach/router": "^1.3.1",
    "amazon-cognito-identity-js": "^3.2.6",
    "eri": "^17.0.0",
    "idb-keyval": "^3.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^13.9.5",
    "@types/reach__router": "^1.3.1",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "favicons": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "prettier": "^2.0.3",
    "typescript": "^3.7.4"
  }
}
