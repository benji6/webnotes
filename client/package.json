{
  "name": "@webnotes/client",
  "version": "1.0.0",
  "repository": "https://github.com/benji6/webnotes",
  "author": "benji6",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheList",
    "build:js": "CACHE_LIST=CACHE_LIST parcel build --experimental-scope-hoisting src/index.html",
    "build:robots": "touch dist/robots.txt",
    "deploy": "yarn build && node scripts/deploy",
    "icons": "node scripts/icons",
    "start": "parcel src/index.html",
    "test": "run-p test:*",
    "test:format": "prettier --check \"src/**/*\"",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "amazon-cognito-identity-js": "^3.0.9",
    "eri": "^7.13.0",
    "final-form": "^4.12.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-final-form": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^11.13.5",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "aws-sdk": "^2.418.0",
    "favicons": "^5.3.0",
    "mime": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  }
}
