{
  "version": "1.0.0",
  "repository": "https://github.com/benji6/webnotes",
  "author": "benji6",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheLists",
    "build:js": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') CACHE_FIRST_CACHE=CACHE_FIRST_CACHE NETWORK_FIRST_CACHE=NETWORK_FIRST_CACHE parcel build --no-source-maps src/index.html",
    "build:robots": "touch dist/robots.txt",
    "fmt": "prettier -u --write '**/*'",
    "icons": "node scripts/icons",
    "start": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') parcel src/index.html --open -p 1236",
    "test": "run-p test:*",
    "test:audit": "npm audit --audit-level=low",
    "test:fmt": "prettier --check -u '**/*'",
    "test:lint": "eslint --ext js,ts,tsx .",
    "test:types": "tsc --noEmit"
  },
  "browserslist": "since 2022 and >1%",
  "dependencies": {
    "@sentry/react": "^8.13.0",
    "amazon-cognito-identity-js": "^6.3.6",
    "date-fns": "^3.0.0",
    "eri": "^45.0.0",
    "idb-keyval": "^6.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/transformer-webmanifest": "^2.12.0",
    "@types/node": "^22.0.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^7.15.0",
    "buffer": "^5.5.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "favicons": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.7.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "typescript": "^5.5.3"
  }
}
