{
  "version": "1.0.0",
  "repository": "https://github.com/benji6/webnotes",
  "author": "benji6",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:* && node scripts/injectCacheLists.mjs",
    "build:js": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') CACHE_FIRST_CACHE=CACHE_FIRST_CACHE NETWORK_FIRST_CACHE=NETWORK_FIRST_CACHE parcel build src/index.html",
    "build:robots": "echo 'User-agent: GPTBot\nDisallow: /\n\nUser-agent: ChatGPT-User\nDisallow: /\n\nUser-agent: anthropic-ai\nDisallow: /\n\nUser-agent: Claude-Web\nDisallow: /\n\nUser-agent: ClaudeBot\nDisallow: /' > dist/robots.txt",
    "fmt": "npm run test:lint -- --fix && prettier -u --write '**/*'",
    "icons": "node scripts/icons.mjs",
    "start": "BUILD_TIME=$(node -e 'console.log((new Date).toISOString())') parcel src/index.html --open -p 1236",
    "test": "run-p test:*",
    "test:audit": "npm audit --audit-level=low",
    "test:fmt": "prettier --check -u '**/*'",
    "test:lint": "eslint",
    "test:types": "tsc --noEmit"
  },
  "browserslist": "defaults and since 2023",
  "dependencies": {
    "@sentry/react": "^9.15.0",
    "amazon-cognito-identity-js": "^6.3.6",
    "date-fns": "^4.1.0",
    "eri": "^46.2.1",
    "idb-keyval": "^6.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/transformer-webmanifest": "^2.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.10.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.1.3",
    "buffer": "^6.0.3",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "favicons": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.7.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "svgo": "^3.3.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.33.0"
  }
}
